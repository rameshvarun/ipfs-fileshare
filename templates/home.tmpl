    <!-- Dropzone v1.3.12 -->
    <link
      rel="stylesheet"
      type="text/css"
      href="{{ .Gateway }}/ipfs/QmR7TbDrjmnh8BWaQEiCWJvPdeMmSa9Yh4e7hNNmHv2r9B/css/dropzone.css"
