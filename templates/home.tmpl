    <script src="{{ .Gateway }}/ipfs/QmR7TbDrjmnh8BWaQEiCWJvPdeMmSa9Yh4e7hNNmHv2r9B/dropzone.js"></script>

    <!-- JQuery Paste Image Reader -->
